package LinearnoProgramiranje;

//Declare a variable B and assign it a value of 5. Apply the increment operator and print the result, then apply the decrement operator and print the result.

public class IncrementDecrement {

	public static void main(String[] args) {

		int B = 5;

		B++; // applying the increment operator
		System.out.println(B);


		B--; // applying the decrement operator
		System.out.println(B);
	}

}
