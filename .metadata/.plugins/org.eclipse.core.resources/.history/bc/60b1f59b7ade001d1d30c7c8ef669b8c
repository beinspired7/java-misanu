package LinearStructures;

public class AritmeticExpressions2 {

	public static void main(String[] args) {
		
		// a)
		System.out.println(
				"a) Math.log(Math.sqrt(Math.abs(Math.sin(a + Math.exp(x/2)) + Math.cos(b - Math.atan(x/2)))))");
		// b)
		System.out.println("b) Math.log(Math.sqrt(Math.pow(Math.tan(x), 2) + 0.2 + Math.cos(y) / Math.exp(x * y)))");
	}

}
