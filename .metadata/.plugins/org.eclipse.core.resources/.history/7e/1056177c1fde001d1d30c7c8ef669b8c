package LinearnoProgramiranje;

public class ImplicitConversion {

	//Write code that demonstrates implicit conversion, i.e. automatic conversion from a narrower to a wider data type (Widening Casting)
	
	public static void main(String[] args) {
		int i = 10;
		double d = i;
		System.out.println("i " + i);
		System.out.println("d " + d);
	}

}
